
{
    "files.associations":
    {
        "*.sorth": "forth",
        ".sorth_init": "forth",
        "*.h": "cpp"
    },

    "typescript.tsc.autoDetect": "off",

    "search.exclude":
    {
        "strange-fort/out": true
    },

    "cSpell.words":
    [
        "dequoted",
        "doesnt",
        "endcase",
        "endcatch",
        "endof",
        "Forths",
        "npos",
        "readline",
        "sorth",
        "strangeforth",
        "structs",
        "uinteger",
        "unmark"
    ],

    "files.exclude":
    {
        "**/sorth": false,
        "**/test.bin": true,
        "**/dist/": true,
        "**/build/": true,
        "**/strange-forth/node_modules/": true,
        "**/strange-forth/out/": true,
        "**/strange-forth/bin/": true,
        "**/*.vsix": true
    },

    "cSpell.enableFiletypes":
    [
        "forth",
        "strangeforth"
    ]
}
