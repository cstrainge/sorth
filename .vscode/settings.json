
{
    "files.associations":
    {
        "*.f": "strangeforth",
        "*.sorth": "strangeforth",
        ".sorth_init": "strangeforth"
    },

    "typescript.tsc.autoDetect": "off",

    "search.exclude":
    {
        "**/strange-forth/out/": true
    },

    "cSpell.words":
    [
        "backsp",
        "dequoted",
        "doesnt",
        "endcase",
        "endcatch",
        "endof",
        "Forths",
        "libatomic",
        "npos",
        "readline",
        "sftmp",
        "sorth",
        "strangeforth",
        "structs",
        "thedoctor",
        "uinteger",
        "unmark",
        "userprofile",
        "xresloader"
    ],

    "files.exclude":
    {
        "**/sorth": false,
        "**/test.bin": true,
        "**/dist/": true,
        "**/build/": true,
        "**/strange-forth/node_modules/": true,
        "**/strange-forth/out/": true,
        "**/strange-forth/bin/": true,
        "**/*.vsix": true
    },

    "cSpell.enableFiletypes":
    [
        "forth",
        "strangeforth"
    ],

    "cmake.configureOnOpen": false
}
