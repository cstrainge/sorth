
{
    "files.associations":
    {
        "*.dox": "c",
        "*.api": "c",
        "*.h": "cpp",
        "Makefile.am": "makefile",
        "*.scons": "python",
        "*.sorth": "forth",
        ".sorth_init": "forth",
        "filesystem": "cpp"
    },

    "cSpell.words":
    [
        "doesnt",
        "endcase",
        "endcatch",
        "endof",
        "Forths",
        "npos",
        "readline",
        "sorth",
        "structs",
        "uinteger",
        "unmark"
    ],

    "files.exclude":
    {
        "**/sorth": true,
        "**/test.bin": true
    },

    "cSpell.enableFiletypes":
    [
        "forth"
    ]
}
