
{
    "comments": {
        "blockComment": [ "(", ")" ]
    },

    "brackets": [
        [ "{", "}" ],
        [ "[", "]" ]
    ],

    "autoClosingPairs": [
        [ "{", "}"   ],
        [ "[", "]"   ],
        [ "( ", " )"   ],
        [ "\"", "\"" ]
    ],

    "surroundingPairs": [
        [ "{", "}"   ],
        [ "[", "]"   ],
        [ "(", ")" ],
        [ "\"", "\"" ]
    ],

    "folding": {
        "markers": {
            "start": ":",
            "end": ";"
        }
    },

    "wordPattern": "[\\w\\s]+"
}
